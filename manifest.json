{
  "manifest_version": 2,

  "name": "SyncTabs",
  "description": "Transfer your opened tabs between devices. ",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon_st.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
